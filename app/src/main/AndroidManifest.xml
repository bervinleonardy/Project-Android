<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.pembelajaran.gamefonik">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        tools:replace="android:icon"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name=".Daftar_Activity" />
        <activity android:name=".SekitarActivity" />
        <activity android:name=".MusikActivity" />
        <activity android:name=".Login_Activity" />
        <activity android:name=".TesPendengaranActivity" />
        <activity android:name=".KataActivity" />
        <activity android:name=".SukuKataActivity" />
        <activity android:name=".HurufActivity" />
        <activity android:name=".InfoActivity" />
        <activity android:name=".PerkembanganActivity" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".DengarActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainGame" />
        <activity android:name=".BinatangActivity" />
        <activity android:name=".TerapiActivity" />
        <activity android:name=".EditProfilActivity"/>
        <activity android:name=".MainActivity" />
    </application>

</manifest>